{

  "manifest_version": 2,
  "name": "hyu-self-check-extension",
  "description": "do self-check automately",
  "version": "1.0",
  "background":
  {
    "service_worker": "background.js"
  },
  "browser_action":
  {
    "default_popup": "settings.html",
    "default_title": "title"
  },
  "icons":
  {
    "128": "128.png",
    "64" : "64.png"
  },
  "permissions": ["tabs", "*://portal.hanyang.ac.kr/*", "storage"],
  "content_scripts": 
  [ 
    {
    "matches": ["*://portal.hanyang.ac.kr/*"],
    "js": ["start.js"],
    "run_at": "document_end"
    } 
  ]
}